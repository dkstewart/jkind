node select (x: bool; a1, a2: real) returns (res: real; Prop1, Prop2, Prop3: bool);
let
   res = if x then a1 else a2;
   
   Prop1 = (res = a1 or res = a2);
   Prop2 = (x => res = a1);
   Prop3 = (not x => res = a2);
   
